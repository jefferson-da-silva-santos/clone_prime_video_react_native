      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#232F3E"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
